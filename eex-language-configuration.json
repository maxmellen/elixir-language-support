{
    "comments": {
        "blockComment": ["<%#", "%>"]
    },
    "brackets": [
        ["{", "}"],
        ["(", ")"],
        ["[", "]"],
        ["<", ">"]
    ],
    "autoClosingPairs": [
        { "open": "{", "close": "}" },
        { "open": "[", "close": "]" },
        { "open": "(", "close": ")" },
        { "open": "\"", "close": "\"", "notIn": ["string"] },
        { "open": "'", "close": "'", "notIn": ["string", "comment"] },
        { "open": "<<", "close": ">>" },
        { "open": "<%", "close": "%>" }
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["<", ">"],
        ["\"", "\""],
        ["'", "'"]
    ]
}
