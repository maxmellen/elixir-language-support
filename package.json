{
    "name": "elixir-language-support",
    "displayName": "Elixir Language Support",
    "description": "Intelligence-free Elixir language support for VS Code",
    "version": "0.0.1",
    "publisher": "maxmellen",
    "engines": {
        "vscode": "^1.20.0"
    },
    "license": "MIT",
    "homepage": "https://github.com/maxmellen/elixir-language-support/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/maxmellen/elixir-language-support/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/maxmellen/elixir-language-support.git"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "elixir",
            "aliases": ["Elixir", "elixir"],
            "extensions": [".ex",".exs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "elixir",
            "scopeName": "source.elixir",
            "path": "./syntaxes/elixir.tmLanguage.json"
        }],
        "snippets": [{
            "language": "elixir",
            "path": "./snippets/elixir.json"
        }]
    }
}
